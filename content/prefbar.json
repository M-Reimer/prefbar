  "prefbar:button:colors":{
    "label":"Colors",
    "type":"check",
    "prefstring":"browser.display.use_document_colors",
    "topref":"value",
    "frompref":"value"
  },
  "prefbar:button:images":{
    "label":"Images",
    "type":"check",
    "prefstring":"permissions.default.image",
    "topref":"value ? 1 : 2",
    "frompref":"value == 1"
