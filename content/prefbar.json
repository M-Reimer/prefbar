  "prefbar:button:javascript":{
    "label":"JavaScript",
    "type":"check",
    "prefstring":"javascript.enabled",
    "topref":"value",
    "frompref":"value"
  },
  "prefbar:button:Flash":{
    "label":"Flash",
    "type":"extcheck",
