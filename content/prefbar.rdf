<RDF:RDF xmlns:RDF="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:prefbar="http://www.xulplanet.com/rdf/prefbar#">

  <RDF:resource about="urn:prefbar:info" prefbar:formatversion="2"/>

  <RDF:Seq about="urn:prefbar:browserbuttons:enabled">
    <RDF:li><RDF:resource about="urn:prefbar:buttons:colors"
      prefbar:label="Colors"
      prefbar:type="check"
      prefbar:prefstring="browser.display.use_document_colors"
      prefbar:topref="value"
      prefbar:frompref="value"/>
    </RDF:li>
    <RDF:li><RDF:resource about="urn:prefbar:buttons:images"
      prefbar:label="Images"
      prefbar:type="check"
      prefbar:prefstring="permissions.default.image"
      prefbar:topref="value ? 1 : 2"
      prefbar:frompref="value == 1"/>
    </RDF:li>
    <RDF:li><RDF:resource about="urn:prefbar:buttons:javascript"
      prefbar:label="JavaScript"
      prefbar:type="check"
      prefbar:prefstring="javascript.enabled"
      prefbar:topref="value"
      prefbar:frompref="value"/>
    </RDF:li>
    <RDF:li><RDF:resource about="urn:prefbar:buttons:Flash"
      prefbar:label="Flash"
      prefbar:type="extcheck"
      prefbar:setfunction="prefbarSetFlash(value);"
      prefbar:getfunction="value = prefbarGetFlash();"/>
    </RDF:li>
    <RDF:li><RDF:resource about="urn:prefbar:buttons:separator10"
      prefbar:label="------------------"
      prefbar:type="separator"/>
    </RDF:li>
    <RDF:li><RDF:resource about="urn:prefbar:buttons:clearallcache"
      prefbar:label="Clear Cache"
      prefbar:type="button"
      prefbar:onclick="prefbarClearAllCache()"/>
    </RDF:li>
    <RDF:li><RDF:resource about="urn:prefbar:buttons:separator11"
      prefbar:label="------------------"
      prefbar:type="separator"/>
    </RDF:li>
    <RDF:li><RDF:resource about="urn:prefbar:buttons:savepage"
      prefbar:label="Save Page"
      prefbar:type="button"
      prefbar:onclick="prefbarSavePage()"/>
    </RDF:li>
    <RDF:li><RDF:resource about="urn:prefbar:buttons:separator8"
      prefbar:label="------------------"
      prefbar:type="separator"/>
    </RDF:li>
    <RDF:li><RDF:resource about="urn:prefbar:buttons:useragent"
      prefbar:label="User Agent"
      prefbar:type="extlist"
      prefbar:dontupdatelistitems="true"
      prefbar:optionlabel1="Real UA"       prefbar:optionvalue1="PREFBARDEFAULT"
      prefbar:optionlabel2="Moz 1.7.13 Lin" prefbar:optionvalue2="js:useragent=&quot;Mozilla/5.0 (X11; U; Linux i686; en-US; rv:1.7.13) Gecko/20060417&quot;; appname=&quot;Netscape&quot;; appversion=&quot;5.0 (X11; en-US)&quot;; platform=&quot;Linux i686&quot;;"
      prefbar:optionlabel3="NS 4.7 Mac"    prefbar:optionvalue3="Mozilla/4.78 (Macintosh; U; PPC)"
      prefbar:optionlabel4="NS 6.2 Lin"    prefbar:optionvalue4="Mozilla/5.0 (X11; U; Linux i686; en-US; rv:0.9.4) Gecko/20011022 Netscape6/6.2"
      prefbar:optionlabel5="IE 6.0 WinXP"  prefbar:optionvalue5="js:useragent=&quot;Mozilla/4.0 (compatible; MSIE 6.0; Windows NT 5.1)&quot;; appname=&quot;Microsoft Internet Explorer&quot;; appversion=&quot;4.0 (compatible; MSIE 6.0; Windows NT 5.1)&quot;; platform=&quot;Win32&quot;;"
      prefbar:optionlabel6="SM 1.0.8 Lin"    prefbar:optionvalue6="js:useragent=&quot;Mozilla/5.0 (X11; U; Linux i686; en-US; rv:1.8.0.10) Gecko/20070306 SeaMonkey/1.0.8&quot;; appname=&quot;Netscape&quot;; appversion=&quot;5.0 (X11; en-US)&quot;; platform=&quot;Linux i686&quot;;"
      prefbar:optionlabel7="FF 2.0.0.2 Lin"    prefbar:optionvalue7="js:useragent=&quot;Mozilla/5.0 (X11; U; Linux i686; en-US; rv:1.8.1.2) Gecko/20070220 Firefox/2.0.0.2&quot;; appname=&quot;Netscape&quot;; appversion=&quot;5.0 (X11; en-US)&quot;; platform=&quot;Linux i686&quot;;"
      prefbar:optionlabel8="Lynx"          prefbar:optionvalue8="Lynx/2.8.3rel.1 libwww-FM/2.14FM"
      prefbar:setfunction="prefbarSetUseragent(value);">
      <prefbar:getfunction>
var t = new Object;
t.items = items; t.value = value;
prefbarGetUseragent(t);
items = t.items; value = t.value;
      </prefbar:getfunction>
    </RDF:resource></RDF:li>
    <RDF:li><RDF:resource about="urn:prefbar:buttons:spacer0"
      prefbar:label="(spacer)"
      prefbar:type="spacer"/>
    </RDF:li>
    <RDF:li><RDF:resource about="urn:prefbar:buttons:separator6"
      prefbar:label="------------------"
      prefbar:type="separator"/>
    </RDF:li>
    <RDF:li><RDF:resource about="urn:prefbar:buttons:help"
      prefbar:label="PrefBar Help"
      prefbar:type="link"
      prefbar:url="chrome://prefbar/locale/help/index.html"/>
    </RDF:li>
    <RDF:li><RDF:resource about="urn:prefbar:buttons:whatsnew"
      prefbar:label="What's New"
      prefbar:type="link"
      prefbar:url="chrome://prefbar/locale/help/newstuff.html"/>
    </RDF:li>
    <RDF:li><RDF:resource about="urn:prefbar:buttons:separator4"
      prefbar:label="------------------"
      prefbar:type="separator"/>
    </RDF:li>
    <RDF:li><RDF:resource about="urn:prefbar:buttons:customize"
      prefbar:label="Customize"
      prefbar:type="button"
      prefbar:onclick="OpenPrefs();"/>
    </RDF:li>
  </RDF:Seq>


  <RDF:Seq about="urn:prefbar:browserbuttons:disabled">
    <RDF:li><RDF:resource about="urn:prefbar:buttons:back"
      prefbar:label="Back"
      prefbar:type="button"
      prefbar:onclick="BrowserBack();"
      prefbar:observes="canGoBack"/>
    </RDF:li>
    <RDF:li><RDF:resource about="urn:prefbar:buttons:forward"
      prefbar:label="Forward"
      prefbar:type="button"
      prefbar:onclick="BrowserForward();"
      prefbar:observes="canGoForward"/>
    </RDF:li>
    <RDF:li><RDF:resource about="urn:prefbar:buttons:reload"
      prefbar:label="Reload"
      prefbar:type="button"
      prefbar:onclick="if (event.shiftKey) BrowserReloadSkipCache(); else BrowserReload();"/>
    </RDF:li>
    <RDF:li><RDF:resource about="urn:prefbar:buttons:stop"
      prefbar:label="Stop"
      prefbar:type="button"
      prefbar:onclick="BrowserStop();"
      prefbar:observes="canStop"/>
    </RDF:li>
    <RDF:li><RDF:resource about="urn:prefbar:buttons:separator0"
      prefbar:label="------------------"
      prefbar:type="separator"/>
    </RDF:li>
    <RDF:li><RDF:resource about="urn:prefbar:buttons:home"
      prefbar:label="Home"
      prefbar:type="button"
      prefbar:onclick="BrowserHome();"/>
    </RDF:li>
    <RDF:li><RDF:resource about="urn:prefbar:buttons:separator13"
      prefbar:label="------------------"
      prefbar:type="separator"/>
    </RDF:li>
    <RDF:li><RDF:resource about="urn:prefbar:buttons:prefbartabjavascript"
      prefbar:label="Javascript (Tab)"
      prefbar:type="extcheck"
      prefbar:browserbtnupdatefor="tab"
      prefbar:setfunction="prefbarSetTabJavascript(value);"
      prefbar:getfunction="value = prefbarGetTabJavascript();"/>
    </RDF:li>
    <RDF:li><RDF:resource about="urn:prefbar:buttons:prefbartabimages"
      prefbar:label="Images (Tab)"
      prefbar:type="extcheck"
      prefbar:browserbtnupdatefor="tab"
      prefbar:setfunction="prefbarSetTabImages(value);"
      prefbar:getfunction="value = prefbarGetTabImages();"/>
    </RDF:li>
    <RDF:li><RDF:resource about="urn:prefbar:buttons:prefbartabplugins"
      prefbar:label="Plugins (Tab)"
      prefbar:type="extcheck"
      prefbar:browserbtnupdatefor="tab"
      prefbar:setfunction="prefbarSetTabPlugins(value, button, event);"
      prefbar:getfunction="value = prefbarGetTabPlugins();"/>
    </RDF:li>
    <RDF:li><RDF:resource about="urn:prefbar:buttons:separator1"
      prefbar:label="------------------"
      prefbar:type="separator"/>
    </RDF:li>
    <RDF:li><RDF:resource about="urn:prefbar:buttons:fonts"
      prefbar:label="Fonts"
      prefbar:type="check"
      prefbar:prefstring="browser.display.use_document_fonts"
      prefbar:topref="Number(value)"
      prefbar:frompref="Boolean(value)"/>
    </RDF:li>
    <RDF:li><RDF:resource about="urn:prefbar:buttons:systemcolors"
      prefbar:label="System Colors"
      prefbar:type="check"
      prefbar:prefstring="browser.display.use_system_colors"
      prefbar:topref="value"
      prefbar:frompref="value"/>
    </RDF:li>
    <RDF:li><RDF:resource about="urn:prefbar:buttons:animation"
      prefbar:label="Animation"
      prefbar:type="check"
      prefbar:prefstring="image.animation_mode"
      prefbar:topref="value ? 'normal' : 'none'"
      prefbar:frompref="value=='normal'"/>
    </RDF:li>
    <RDF:li><RDF:resource about="urn:prefbar:buttons:java"
      prefbar:label="Java"
      prefbar:type="extcheck"
      prefbar:setfunction="prefbarSetJava(value);"
      prefbar:getfunction="value = prefbarGetJava();"/>
    </RDF:li>
    <RDF:li><RDF:resource about="urn:prefbar:buttons:popups"
      prefbar:label="Popups"
      prefbar:type="check"
      prefbar:prefstring="dom.disable_open_during_load"
      prefbar:topref="!value"
      prefbar:frompref="!value"/>
    </RDF:li>
    <RDF:li><RDF:resource about="urn:prefbar:buttons:proxycheck"
      prefbar:label="Proxies"
      prefbar:type="extcheck"
      prefbar:setfunction="prefbarSetProxyEnabled(value);"
      prefbar:getfunction="value = prefbarGetProxyEnabled();"/>
    </RDF:li>
    <RDF:li><RDF:resource about="urn:prefbar:buttons:proxymenu"
      prefbar:label="Proxy Menulist"
      prefbar:type="menulist"
      prefbar:prefstring="network.proxy.type"
      prefbar:optionlabel1="No Proxies"       prefbar:optionvalue1="0"
      prefbar:optionlabel2="Manual Proxy"     prefbar:optionvalue2="1"
      prefbar:optionlabel3="Automatic Proxy"  prefbar:optionvalue3="2"/>
    </RDF:li>
    <RDF:li><RDF:resource about="urn:prefbar:buttons:cookies"
      prefbar:label="Cookies"
      prefbar:type="check"
      prefbar:prefstring="network.cookie.cookieBehavior"
      prefbar:topref="2*Number(!value)"
      prefbar:frompref="value != 2"/>
    </RDF:li>
    <RDF:li><RDF:resource about="urn:prefbar:buttons:cookieask"
      prefbar:label="Cookie Warning"
      prefbar:type="check"
      prefbar:prefstring="network.cookie.lifetimePolicy"
      prefbar:topref="value ? 1 : 0"
      prefbar:frompref="value==1"/>
    </RDF:li>
    <RDF:li><RDF:resource about="urn:prefbar:buttons:pipelining"
      prefbar:label="Pipelining"
      prefbar:type="extcheck"
      prefbar:setfunction="prefbarSetPipelining(value);"
      prefbar:getfunction="value = prefbarGetPipelining();"/>
    </RDF:li>
    <RDF:li><RDF:resource about="urn:prefbar:buttons:diskcache"
      prefbar:label="Disk Cache"
      prefbar:type="check"
      prefbar:prefstring="browser.cache.disk.enable"
      prefbar:topref="value"
      prefbar:frompref="value"/>
    </RDF:li>
    <RDF:li><RDF:resource about="urn:prefbar:buttons:memcache"
      prefbar:label="Mem Cache"
      prefbar:type="check"
      prefbar:prefstring="browser.cache.memory.enable"
      prefbar:topref="value"
      prefbar:frompref="value"/>
    </RDF:li>
    <RDF:li><RDF:resource about="urn:prefbar:buttons:referrer"
      prefbar:label="Send Referrer"
      prefbar:type="check"
      prefbar:prefstring="network.http.sendRefererHeader"
      prefbar:topref="2*Number(value)"
      prefbar:frompref="Boolean(value)"/>
    </RDF:li>
    <RDF:li><RDF:resource about="urn:prefbar:buttons:separator2"
      prefbar:label="------------------"
      prefbar:type="separator"/>
    </RDF:li>
    <RDF:li><RDF:resource about="urn:prefbar:buttons:clearmemcache"
      prefbar:label="Clear Mem Cache"
      prefbar:type="button"
      prefbar:onclick="prefbarClearMemCache()"/>
    </RDF:li>
    <RDF:li><RDF:resource about="urn:prefbar:buttons:cleardiskcache"
      prefbar:label="Clear Disk Cache"
      prefbar:type="button"
      prefbar:onclick="prefbarClearDiskCache()"/>
    </RDF:li>
    <RDF:li><RDF:resource about="urn:prefbar:buttons:clearhist"
      prefbar:label="Clear History"
      prefbar:type="button"
      prefbar:onclick="prefbarClearHistory()"/>
    </RDF:li>
    <RDF:li><RDF:resource about="urn:prefbar:buttons:clearurlbar"
      prefbar:label="Clear Location Bar"
      prefbar:type="button"
      prefbar:onclick="prefbarClearLocationBar()"/>
    </RDF:li>
    <RDF:li><RDF:resource about="urn:prefbar:buttons:clearcookies"
      prefbar:label="Clear Cookies"
      prefbar:type="button"
      prefbar:onclick="prefbarClearCookies()"/>
    </RDF:li>
    <RDF:li><RDF:resource about="urn:prefbar:buttons:clearall"
      prefbar:label="Clear All"
      prefbar:type="button">
      <prefbar:onclick>
//### DO NOT EDIT THIS BUTTON! ###
// This button will be overwritten with each PrefBar update!
// If you want to customize this button, create a copy first.
// On your copy, you may (un)comment below, as desired

prefbarClearLocationBar();
prefbarClearHistory();
prefbarClearAllCache();
//prefbarClearCookies();
//prefbarClearFormData();
//prefbarClearDownloads();
//prefbarClearPasswords();
//prefbarClearSessions();
//prefbarClearOfflineApps();
      </prefbar:onclick>
    </RDF:resource></RDF:li>
    <RDF:li><RDF:resource about="urn:prefbar:buttons:separator5"
      prefbar:label="------------------"
      prefbar:type="separator"/>
    </RDF:li>
    <RDF:li><RDF:resource about="urn:prefbar:buttons:resizelist"
      prefbar:label="Resize"
      prefbar:type="extlist"
      prefbar:dontupdatelistitems="true"
      prefbar:optionlabel1="640x480"  prefbar:optionvalue1="640x480"
      prefbar:optionlabel2="800x600"  prefbar:optionvalue2="800x600"
      prefbar:optionlabel3="1024x768" prefbar:optionvalue3="1024x768"
      prefbar:setfunction="prefbarSetResolution(value);"
      prefbar:getfunction="value = prefbarGetResolution();"/>
    </RDF:li>
    <RDF:li><RDF:resource about="urn:prefbar:buttons:separator9"
      prefbar:label="------------------"
      prefbar:type="separator"/>
    </RDF:li>
    <RDF:li><RDF:resource about="urn:prefbar:buttons:newtab"
      prefbar:label="New Tab"
      prefbar:type="button"
      prefbar:onclick="BrowserOpenTab();"/>
    </RDF:li>
    <RDF:li><RDF:resource about="urn:prefbar:buttons:prefbarrestoretab"
      prefbar:label="Restore Tab"
      prefbar:type="button"
      prefbar:onclick="prefbarRestoreTab();"/>
    </RDF:li>
    <RDF:li><RDF:resource about="urn:prefbar:buttons:killflash"
      prefbar:label="Kill Flash"
      prefbar:type="button"
      prefbar:onclick="prefbarKillFlash()"/>
    </RDF:li>
    <RDF:li><RDF:resource about="urn:prefbar:buttons:separator12"
      prefbar:label="------------------"
      prefbar:type="separator"/>
    </RDF:li>
    <RDF:li><RDF:resource about="urn:prefbar:buttons:fontdown"
      prefbar:label="Font -"
      prefbar:type="button"
      prefbar:onclick="prefbarZoomReduce();"/>
    </RDF:li>
    <RDF:li><RDF:resource about="urn:prefbar:buttons:fontup"
      prefbar:label="Font +"
      prefbar:type="button"
      prefbar:onclick="prefbarZoomEnlarge();"/>
    </RDF:li>
    <RDF:li><RDF:resource about="urn:prefbar:buttons:separator3"
      prefbar:label="------------------"
      prefbar:type="separator"/>
    </RDF:li>
    <RDF:li><RDF:resource about="urn:prefbar:buttons:prefbarhomepagelink"
      prefbar:label="PrefBar homepage"
      prefbar:type="link"
      prefbar:url="http://prefbar.mozdev.org"/>
    </RDF:li>
    <RDF:li><RDF:resource about="urn:prefbar:buttons:mozilla.org"
      prefbar:label="mozilla.org"
      prefbar:type="link"
      prefbar:url="http://www.mozilla.org"/>
    </RDF:li>
    <RDF:li><RDF:resource about="urn:prefbar:buttons:mozillazine.org"
      prefbar:label="MozillaZine.org"
      prefbar:type="link"
      prefbar:url="http://www.mozillazine.org"/>
    </RDF:li>
    <RDF:li><RDF:resource about="urn:prefbar:buttons:separator7"
      prefbar:label="------------------"
      prefbar:type="separator"/>
    </RDF:li>
    <RDF:li><RDF:resource about="urn:prefbar:buttons:hidetoolbar"
      prefbar:label="Hide Toolbar"
      prefbar:type="button"
      prefbar:onclick="setTimeout(TogglePrefBar);"/>
    </RDF:li>
    <RDF:li><RDF:resource about="urn:prefbar:buttons:proxylist"
      prefbar:label="Proxy Serverlist"
      prefbar:type="extlist"
      prefbar:dontupdatelistitems="true"
      prefbar:optionlabel1="None"         prefbar:optionvalue1=""
      prefbar:optionlabel2="Proxy1"       prefbar:optionvalue2="testproxy:8080"
      prefbar:optionlabel3="Proxy2"       prefbar:optionvalue3="testproxy2:80"
      prefbar:setfunction="prefbarSetProxy(value);"
      prefbar:getfunction="value=prefbarGetProxy();"/>
    </RDF:li>
    <RDF:li><RDF:resource about="urn:prefbar:buttons:prefbarlanglist"
      prefbar:label="Language"
      prefbar:type="extlist"
      prefbar:optionlabel1="Don't edit here. Use the content language GUI of the browser to add new languages"   prefbar:optionvalue1=""
      prefbar:setfunction="prefbarSetLanguage(value);">
      <prefbar:getfunction>
var t = new Object;
t.items = items; t.value = value;
prefbarGetLanguage(t);
items = t.items; value = t.value;
      </prefbar:getfunction>
    </RDF:resource></RDF:li>
    <RDF:li><RDF:resource about="urn:prefbar:buttons:executedemo1"
      prefbar:label="executedemo1"
      prefbar:type="button"
      prefbar:onclick="prefbarExecute(&quot;C:\\Windows\\notepad.exe&quot;);"/>
    </RDF:li>
    <RDF:li><RDF:resource about="urn:prefbar:buttons:executedemo2"
      prefbar:label="executedemo2"
      prefbar:type="button">
      <prefbar:onclick>
var Arguments = new Array
Arguments[0] = "/c"
Arguments[1] = "dir"
Arguments[2] = "&amp;pause"
prefbarExecute("C:\\Windows\\System32\\cmd.exe", Arguments);
delete Arguments;
      </prefbar:onclick>
    </RDF:resource></RDF:li>
  </RDF:Seq>

</RDF:RDF>
